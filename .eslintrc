{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "describe": true,
    "test": true,
    "expect": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-console": 1
  },
  "plugins": [
    "react"
  ]
}